;; -*- mode: fennel; -*- vi:ft=fennel
;; Configuration file for Fenneldoc v0.1.7
;; https://gitlab.com/andreyorst/fenneldoc

{:fennel-path {}
 :function-signatures true
 :ignored-args-patterns ["%.%.%." "%_" "c" "s" "x" "f"]
 :inline-references "link"
 :insert-comment true
 :insert-copyright true
 :insert-license true
 :insert-version true
 :keys {:copyright "_COPYRIGHT"
        :description "_DESCRIPTION"
        :doc-order "_DOC_ORDER"
        :license "_LICENSE"
        :module-name "_MODULE_NAME"
        :version "_VERSION"}
 :mode "checkdoc"
 :order "alphabetic"
 :out-dir "./doc"
 :project-copyright "Copyright (C) 2021 Andrey Listopadov"
 :project-doc-order {:init-macros.fnl ["lazy-seq" "lazy-cat"]
                     :init.fnl ["cons"
                                "first"
                                "rest"
                                "next"
                                "seq"
                                "lazy-seq"
                                "list"
                                "dorun"
                                "doall"
                                "realized?"
                                "pack"
                                "unpack"]}
 :project-license "[MIT](https://gitlab.com/andreyorst/lazy-seq/-/raw/master/LICENSE)"
 :project-version "v0.0.2"
 :sandbox false
 :test-requirements {:init-macros.fnl "(require-macros :fennel-test.test)
                                       (require-macros :init-macros)
                                       (local {: cons : map : take : range : rest : drop : first : unpack} (require :init))"
                     :init.fnl "(require-macros :fennel-test.test)"}
 :toc true}
